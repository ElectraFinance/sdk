{
  "$schema": "https://json.schemastore.org/package",
  "name": "@electra.finance/sdk",
  "version": "0.1.50",
  "description": "Electra finance SDK",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "npm run build && node lib/esm/index.js",
    "develop": "concurrently -i -k -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"yarn watch-js\" \"yarn watch-ts\"",
    "clean": "rimraf lib/*",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon lib/esm/index.js",
    "prepare": "is-ci || husky install",
    "build": "tsup src/index.ts",
    "coverage": "jest --coverage",
    "lint:eslint": "eslint ./src --ext .ts,.js,.tsx,.jsx",
    "lint:eslint:fix": "eslint ./src --ext .ts,.js,.tsx,.jsx --fix",
    "test": "dotenv jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "examples:liquidation": "ts-node-esm ./examples/liquidation.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ElectraFinance/sdk.git"
  },
  "keywords": [
    "sdk",
    "electra",
    "electrafinance",
    "trading"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ElectraFinance/sdk/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/strictest": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.4.1",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "http-terminator": "^3.2.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "github:TypeStrong/ts-node#main",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@electra.finance/contracts": "0.1.16",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "bignumber.js": "^9.1.1",
    "bson-objectid": "^2.0.4",
    "buffer": "^6.0.3",
    "ethers": "^6.6.4",
    "express": "^4.18.2",
    "isomorphic-ws": "^5.0.0",
    "just-clone": "^6.2.0",
    "merge-anything": "^5.1.7",
    "nanoevents": "^7.0.1",
    "neverthrow": "^6.0.0",
    "simple-typed-fetch": "0.2.1",
    "stream-browserify": "^3.0.0",
    "tiny-invariant": "^1.3.1",
    "ts-is-present": "^1.2.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "zod": "3.21.4"
  },
  "homepage": "https://github.com/ElectraFinance/sdk#readme",
  "files": [
    "lib/**/*"
  ],
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  }
}
